{
  "manifest_version": 3,
  "name": "Tyutorlar",
  "version": "1.0",
  "description": "Tyturolarga biriktirilgan guruhlar haqida ma'lumotlar olish",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tyutordagi guruh davomatini olish"
  },
  "content_scripts": [
    {
      "matches": ["https://hemis.jbnuu.uz/*"],
      "js": ["content.js"]
    }
  ]
}